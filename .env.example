# Development variables for the Dijital Sosyal Hak Platformu
# Ensure you set these values appropriately in production.

DEVCYCLE_API_KEY=your_devcyle_api_key_here

# Database credentials (do not expose to public repositories)
DB_USER=your_db_user_here        # Change to your database user
DB_PASSWORD=your_db_password_here  # Change to your database password
DB_HOST=localhost                 # Change to your database host
DB_NAME=your_db_name_here         # Change to your database name

# JWT Secret for token generation
JWT_SECRET=your_jwt_secret_here   # Change to your JWT secret

# Security Headers Configuration
SECURE_HEADERS=your_secure_headers_here # Add your security headers here